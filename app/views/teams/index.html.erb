<div class="container margin-top-30">
  <% @teams.each do |team| %>
  <div class="product">
    <div class='product-upvote'>
      <div class='product-count'>5</div>
    </div>
    <img src="https://picsum.photos/200" alt="kudoz" class="product-image hidden-xs">
    <div class='product-body'>
     <%= link_to team_path(team), class: "hidden-link" do %>
      <h3><%= team.name %></h3>
    <% end %>
      <ul class="list list-inline">
        <% team.coach_functions.each do |coach| %>
        <li> <%= coach.coaches.first.user.f_name %> <%= coach.coach_role %> </li>
        <li>   </li>
        <% end %>
      </ul>
    </div>
    <ul class="list-inline product-controls product-controls-hidden hidden-sm hidden-xs">
      <li><a href=""><i class="fa fa-heart"></i></a></li>
      <li><a href=""><i class="fa fa-share"></i></a></li>
      <li><a href=""><i class="fa fa-star"></i></a></li>
    </ul>
  <% end %>
  </div>
</div>

